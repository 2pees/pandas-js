{
  "extends": "airbnb",
  "rules": {
    "curly": "off",
    "indent": [
      "warn",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "camelcase": "off",
    "no-undef": "off",
    "import/extensions": "off",
    "no-underscore-dangle": "off",
    "object-curly-spacing": "off",
    "no-restricted-syntax": 0,
    "operator-linebreak": [
      "error",
      "before"
    ],
    "quotes": "off"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    }
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "parser": "babel-eslint",
  "root": true
}
